@model IEnumerable<NSPecor.Models.MUB_ESTADO_SUB>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Listado de Estados de Subestación</h2>
<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NOM_ESTADO_SUB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACTIVO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NOM_ESTADO_SUB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ACTIVO)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID_ESTADO_SUB }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.ID_ESTADO_SUB }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.ID_ESTADO_SUB })
        </td>
    </tr>
}

</table>
