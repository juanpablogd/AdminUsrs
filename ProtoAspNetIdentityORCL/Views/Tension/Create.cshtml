@model NSPecor.Models.MUB_TENSION

@{
    ViewBag.Title = "Crear";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Crear</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>TENSIÓN</h4>
        <hr />
        @Html.ValidationSummary(true)

        <div class="form-group">
            @Html.LabelFor(model => model.ID_NIVEL_TENSION, "ID_NIVEL_TENSION", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("ID_NIVEL_TENSION", String.Empty)
                @Html.ValidationMessageFor(model => model.ID_NIVEL_TENSION)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.DESCRIPCION, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.DESCRIPCION)
                @Html.ValidationMessageFor(model => model.DESCRIPCION)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Crear" class="btn btn-default" />
            </div>
        </div>
    </div>
}
<div>
    @Html.ActionLink("Ver Listado", "Index")
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
