@model IEnumerable<NSPecor.Models.MUB_NIVEL_TENSION>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Listado de Nivel de tensión</h2>
<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DESCRIPCION)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DESCRIPCION)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID_NIVEL_TENSION }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.ID_NIVEL_TENSION }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.ID_NIVEL_TENSION })
        </td>
    </tr>
}

</table>
